<div class="col-md-6">
  <h4>Hello <%= current_user.profile.first_name %> <%= current_user.profile.last_name %></h4>
<%= simple_form_for [@company] do |f| %>
  <%= f.input :name, label: "Name of the company or subsidiary" %>
  <%= f.input :street_number %>
  <%= f.input :street %>
  <%= f.input :picture, as: :attachinary %>
  </div>
  <div class="col-md-6">
    <br>
    <h1>HELLO</h1>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :email %>
  <%= f.submit company_message, class: "btn rounded-camp" %>
<% end %>
</div>

